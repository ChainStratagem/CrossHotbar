    </Scripts>
  </Frame>
  
  <Frame name="WXHBCrosshotbarTemplate" virtual="true" inherits="SecureHandlerStateTemplate" parent="UIParent" frameStrata="MEDIUM" hidden="true" mixin="CrossHotbarMixin">
    <Size x="554" y="120"/>
    <Scripts>
